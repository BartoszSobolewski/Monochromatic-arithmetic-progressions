Cell[BoxData[
 RowBox[{
  RowBox[{"Lemma34", "[", 
   RowBox[{"p_", ",", "s_", ",", "i_", ",", "j_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"P", ",", "S", ",", "Sigma", ",", "Gamma"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "we", " ", "operate", " ", "on", " ", "integers", " ", "rather", " ", 
      "than", " ", "strings"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"P", "=", 
      RowBox[{"String2Int", "[", "p", "]"}]}], ";", " ", 
     RowBox[{"S", "=", 
      RowBox[{"String2Int", "[", "s", "]"}]}], ";", "     ", 
     RowBox[{"(*", " ", 
      RowBox[{"integers", " ", "represented", " ", "by", " ", 
       StyleBox["p",
        FontWeight->"Bold"], " ", "and", " ", 
       StyleBox["s",
        FontWeight->"Bold"]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Sigma", "=", 
      RowBox[{"Mod", "[", 
       RowBox[{
        RowBox[{"j", "*", "S"}], ",", 
        RowBox[{"2", "^", 
         RowBox[{"StringLength", "[", "s", "]"}]}]}], "]"}]}], ";", "    ", 
     RowBox[{"(*", " ", 
      RowBox[{"integer", " ", "represented", " ", "by", " ", 
       StyleBox["\[Sigma]",
        FontWeight->"Bold"], " ", "from", " ", "the", " ", "lemma"}], " ", 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Gamma", "=", 
      RowBox[{"P", "+", 
       RowBox[{
        RowBox[{"2", "^", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"StringLength", "[", "p", "]"}], "-", "i"}], ")"}]}], 
        "Sigma"}]}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{"integer", " ", "represented", " ", "by", " ", 
       StyleBox["\[Gamma]",
        FontWeight->"Bold"], " ", "from", " ", "the", " ", "lemma"}], " ", 
      "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"assumption", " ", 
         RowBox[{"(", "b", ")"}], " ", "is", " ", "satisfied", " ", "if", " ",
          "and", " ", "only", " ", "if", " ", "the"}], " ", "|", "s", "|", 
        RowBox[{
        "th", " ", "least", " ", "significant", " ", "binary", " ", "digit", 
         " ", "Sigma", " ", "and", " ", 
         RowBox[{"(", 
          RowBox[{"|", "s", "|", 
           RowBox[{"+", 
            RowBox[{"|", "p", "|", 
             RowBox[{"-", "i"}]}]}]}], ")"}], "th", " ", "digit", " ", "of", 
         " ", "Gamma", " ", "are", " ", "the", " ", "same"}]}], ",", " ", 
       RowBox[{
       "which", " ", "is", " ", "further", " ", "equivalent", " ", "to", " ", 
        "the", " ", "following", " ", "equality"}]}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"(", " ", 
         RowBox[{
          RowBox[{"Floor", "[", 
           RowBox[{"Sigma", "/", 
            RowBox[{"2", "^", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"StringLength", "[", "s", "]"}], "-", "1"}], ")"}]}]}],
            "]"}], "==", 
          RowBox[{"Floor", "[", 
           RowBox[{"Gamma", "/", 
            RowBox[{"2", "^", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"StringLength", "[", "s", "]"}], "+", 
               RowBox[{"StringLength", "[", "p", "]"}], "-", "i", "-", "1"}], 
              ")"}]}]}], "]"}]}], " ", ")"}], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"assumption", " ", 
          RowBox[{"(", "c", ")"}]}], " ", "*)"}], "\[IndentingNewLine]", " ", 
        "&&", " ", 
        RowBox[{
         RowBox[{"Mod", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"r", "[", "P", "]"}], "+", 
            RowBox[{"r", "[", "Sigma", "]"}], "+", 
            RowBox[{"r", "[", "Gamma", "]"}]}], ",", "2"}], "]"}], "\[Equal]",
          "1"}]}], ",", " ", "\[IndentingNewLine]", "True", ",", " ", 
       "False"}], "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 InitializationCell->True,
